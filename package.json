{
  "name": "gg-finance-bot",
  "version": "1.0.0",
  "description": "Criar um assistente financeiro inteligente via WhatsApp para registrar despesas/receitas, gerar insights, e validar adesão dos usuários em um MVP rápido, escalável e com boa performance.",
  "main": "index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "db:pull": "dotenv -e .env -- prisma db pull",
    "db:reset": "dotenv -e .env -- prisma migrate reset",
    "db:migrate": "dotenv -e .env -- prisma migrate dev",
    "db:generate": "dotenv -e .env -- prisma generate",
    "db:deploy": "npm run db:migrate && npm run db:generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/rate-limit": "^10.3.0",
    "@google/generative-ai": "^0.24.1",
    "@prisma/client": "^6.19.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.2",
    "fastify-cors": "^6.0.3",
    "fastify-rate-limit": "^5.8.0",
    "fastify-sensible": "^3.1.2",
    "mysql2": "^3.15.3",
    "node-cron": "^4.2.1",
    "node-fetch": "^3.3.2",
    "prisma": "^6.19.0",
    "puppeteer": "^24.29.1",
    "qrcode": "^1.5.4",
    "qrcode-terminal": "^0.12.0",
    "whatsapp-web.js": "^1.34.2",
    "ws": "^8.18.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "@types/qrcode": "^1.5.6",
    "@types/qrcode-terminal": "^0.12.2",
    "dotenv-cli": "^11.0.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}